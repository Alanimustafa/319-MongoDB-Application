<html>
                <head>
                    <title><%= vehicle.type %> Inventory</title>
                </head>
                <body style="background-color:#333f3c;">
                    <h1 style="color:yellow"><%= vehicle.type %> Inventory</h1>
                    <a style="color:orange" href="http://localhost:3000/sedans/">SEDAN  |</a>
                    <a style="color:orange" href="http://localhost:3000/suvs/">  SUV  |</a>
                    <a style="color:orange" href="http://localhost:3000/trucks/">TRUCK</a>
                    <br> 
                    <a style="color:orange" href="http://localhost:3000/">Back</a>
                    <br>
                    <a style="color:orange" href="http://localhost:3000/">HOME</a>

                    <br>
                    <div style="color:white">
                        <h2> <%= vehicle.type %> Inventory</h2>
                        <!-- Looping through the objectID -->
                        <ul><% vehicle.forEach(vehicle=> { %>
                            <li><a href="/sedans/<%= vehicle._id %>" style="color:orange; text-decoration: none; " >
                                    <%= vehicle.year + ' - ' + vehicle.make + ' - ' + vehicle.model %>
                                </a></li>
                            <% }); %>
                        </ul>
                        <a href="/sedans/new" style="color:orange">New SEDAN</a>
                        <a href="/suvs/new" style="color:orange">New SUV</a>
                        <a href="/trucks/new" style="color:orange">New TRUCK</a>
                    </div>
                </body>
            </html>